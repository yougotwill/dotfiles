ios2usb () {
  dd if="$1" of="$2" bs=1m status=progress
}

tx () {
  tmux new -A -s "$1" "$1"
}

txx () {
  tmux new -A -s "$1"
}

webtorrentplayer () {
  webtorrent download $1 --interactive-select --out ~/Downloads/_Torrents --iina
}

hl () {
  ${@:2} | bat --paging=never -l $1
}

write2File () {
  ${@:2} > $1 
}

lk () {
  cd "$(walk "$@")"
}

setTitleBarDark () {
defaults write $1 NSRequiresAquaSystemAppearance 0
echo "set dark title bar for $1"
}
